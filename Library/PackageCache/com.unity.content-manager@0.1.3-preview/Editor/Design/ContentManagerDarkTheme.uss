.flat-border {
    border-left-color: rgb(48, 48, 48);
    border-right-color: rgb(48, 48, 48);
    border-top-color: rgb(48, 48, 48);
    border-bottom-color: rgb(48, 48, 48);
}

.button-primary {
    background-color: rgb(61, 119, 194);
}

.main-text {
    color: rgb(255, 255, 255);
    white-space: normal;
}

.deep-background {
    background-color: rgb(25, 25, 25);
    color: rgb(152, 152, 152);
}

.shallow-background {
    background-color: rgb(56, 56, 56);
}

.raised-background {
    background-color: rgb(38, 38, 38);
    color: rgb(152, 152, 152);
}

.raised-background.flat-border {
    border-left-color: rgb(60, 60, 60);
    border-right-color: rgb(60, 60, 60);
    border-top-color: rgb(60, 60, 60);
    border-bottom-color: rgb(60, 60, 60);
}

.selected.content-entry {
    border-left-color: rgb(61, 119, 194);
    border-right-color: rgb(61, 119, 194);
    border-top-color: rgb(61, 119, 194);
    border-bottom-color: rgb(61, 119, 194);
}

.selected > .raised-background {
}

.selected.category {
    background-color: rgb(63, 63, 63);
    color: rgb(255, 255, 255);
}

.selected > .category-label {
    color: rgb(255, 255, 255);
}

.error {
    -unity-font-style: bold;
    color: rgb(176, 0, 0);
}

.download-mark {
    color: rgb(49, 49, 49);
}

#DescriptionLocation {
    color: rgb(102, 102, 102);
}

#DescriptionStatus {
    color: rgb(102, 102, 102);
}
